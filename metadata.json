{
    "name": "vankosoft/vs_dotnet",
    "version": "0.1.0",
    "author": "Ivan I. Atanasov",
    "summary": "Installs, configures, and manages the dotnet core and mono.",
    "license": "Apache-2.0",
    "source": "https://github.com/iatanasov77/puppet-dotnet.git",
    "project_page": "https://github.com/iatanasov77/puppet-dotnet",
    "dependencies": [
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 4.13.1 < 7.0.0"
        },
        {
            "name": "vankosoft/vs_devenv",
            "version_requirement": ">= 0.1.0"
        },
        {
            "name": "vankosoft/vs_lamp",
            "version_requirement": ">= 0.1.0"
        }
    ],
    "operatingsystem_support": [
        {
            "operatingsystem": "RedHat",
            "operatingsystemrelease": [
                "7",
                "8"
            ]
        },
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": [
                "7",
                "8"
            ]
        }
    ],
    "requirements": [
        {
            "name": "puppet",
            "version_requirement": ">= 8.0.0 < 9.0.0"
        }
    ],
    "description": "Install DotNet SDK"
}
